  "license": "MIT",
  "funding": "https://github.com/sponsors/Lumirelle",
  "homepage": "https://github.com/Lumirelle/profiles",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lumirelle/profiles.git"
  },
  "bugs": "https://github.com/Lumirelle/profiles/issues",
  "exports": {
    ".": "./dist/index.mjs",
